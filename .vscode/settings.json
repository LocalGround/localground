// Place your settings in this file to overwrite default and user settings.
{
    "workbench.statusBar.visible": true,
    "editor.wordWrap": "on",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Hybrid Next",
    "workbench.colorCustomizations": {
        "activityBar.background": "#1C252A",
        "list.inactiveSelectionBackground": "#53606d",
        "list.activeSelectionBackground": "#53606d",
        "list.inactiveSelectionForeground": "#B3BA3D",
        "list.activeSelectionForeground": "#B3BA3D"
        
        
    },
    "python.pythonPath": "/usr/bin/python",
    "liveSassCompile.settings.formats": [
        {
          "format": "expanded",
          "extensionName": ".css",
          "savePath": "~/../"
        }
      ]  
}